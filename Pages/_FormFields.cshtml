@model IEnumerable<FormField>
@{
    var fieldsValues = TempData["FieldsValues"] as List<FormData>;


}
@if (Model.Any())
{
    <table class="table table-striped text-right">
        <thead>
            <tr>
                @foreach (var item in Model)
                {
                    <th>@item.Title</th>
                }
            </tr>
        </thead>
        <tbody>


            @foreach (var formData in fieldsValues)
            {
                <tr>
                    @{
                        var formValueObj = Newtonsoft.Json.Linq.JObject.Parse(formData.FormValue);

                        foreach (var item in formValueObj)
                        {
                            <td>@item.Value</td>
                        }
                    }

                </tr>
            }

        </tbody>
    </table>
}
else
{
    <div class="alert alert-primary text-center">
        <p>برای این فرم مقداری ثبت نشده است !</p>
    </div>
}
